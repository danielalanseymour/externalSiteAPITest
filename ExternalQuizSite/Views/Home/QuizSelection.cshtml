@model ExternalQuizSite.Models.ModelHome

    <!-- using WebApplication1.Models -->
@{
    //TempData["QuizSelectionView"] = Model;
}

<head>
    <title> Quiz Selectoin </title>
</head>
<body>

    <center id="quizSelection">
        <h2>QuizSelection</h2>
        @using (Html.BeginForm("GoToQuiz", "Home")){ 
            foreach (String quiz in Model.Quizzes) {
                <button name="quizName" value="@quiz" id="quizSelection" title="Take @quiz and have fun"> 
                    @quiz  
                </button> <br/>
            }
        }
    </center>


</body>
